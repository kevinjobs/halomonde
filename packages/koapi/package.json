{
  "name": "koapi",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src/**/* -e ts src/index.ts",
    "prod": "cross-env NODE_ENV=production node ./dist/index.js",
    "build": "rimraf ./dist/* && tsc",
    "compile": "tsc",
    "test": "cross-env NODE_ENV=test mocha --require ts-node/register test/**/*.ts --reporter mochawesome --exit",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=html yarn test",
    "db:migrate": "node migrate.js"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@koa/router": "10.0.0",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "axios": "0.21.1",
    "dayjs": "1.10.4",
    "http-status": "^1.5.0",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "rimraf": "^3.0.2",
    "sequelize": "6.6.5",
    "sqlite3": "5.0.2",
    "uuid": "^8.3.2",
    "validate": "^5.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "3.0.2",
    "@types/koa__router": "8.0.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.14.25",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.4",
    "cross-env": "7.0.3",
    "mocha": "^9.1.3",
    "mochawesome": "^7.0.1",
    "nodemon": "2.0.7",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.1.6",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
}
